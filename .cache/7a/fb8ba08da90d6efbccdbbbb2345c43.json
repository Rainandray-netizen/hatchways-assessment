al":{"line":52,"column":4}},{"generated":{"line":81,"column":21},"source":"components/App/StudentCard.js","original":{"line":69,"column":6}},{"generated":{"line":81,"column":61},"source":"components/App/StudentCard.js","original":{"line":52,"column":4}},{"generated":{"line":81,"column":62},"source":"components/App/StudentCard.js","original":{"line":51,"column":2}},{"generated":{"line":82,"column":0},"source":"components/App/StudentCard.js","original":{"line":72,"column":1}},{"generated":{"line":82,"column":1},"source":"components/App/StudentCard.js","original":{"line":5,"column":0}},{"name":"StudentCard","generated":{"line":84,"column":15},"source":"components/App/StudentCard.js","original":{"line":74,"column":15}}],"sources":{"components/App/StudentCard.js":"import React, { useState } from 'react'\nimport FontAwesome from 'react-fontawesome'\nimport TagInput from './TagInput'\n\nconst StudentCard = ({student, updateTags, index}) => {\n  const {\n    city,\n    company,\n    email,\n    firstName,\n    grades,\n    lastName,\n    pic,\n    skill,\n    tags\n  } = student\n\n  const [expand, toggleExpand] = useState(false)\n\n  const handleClick = () => {\n    toggleExpand(!expand)\n  }\n\n  const handleSubmit = (newTag) => {\n    updateTags(index, newTag)\n  }\n\n  //string to integer conversion\n  const gradesInt = grades.map((grade)=>parseInt(grade))\n\n  const gradeAvg = gradesInt.reduce((a, b) => a + b, 0)/grades.length\n  \n  const ExpandList = () => {\n    return(\n      <>\n        <div className='gradelist'>\n          {grades.map((grade, index)=>{\n            return <p>{`Test ${index+1}: \\xa0\\xa0\\xa0\\xa0\\xa0\\xa0 ${grade}%`}</p>\n          })}\n        </div>\n        <div className='tags-container'>\n          { tags.map((tag)=>{\n            return <span className='tag'>{tag}</span>\n          }) }\n        </div>\n        <TagInput handleSubmit={handleSubmit}/>\n      </>\n    )\n  }\n\n  return (\n    <article>\n      <div className='card-container'>\n        <div className='card-info'>\n          <img src={pic}/>\n          <div>\n            <h1>{`${firstName} ${lastName}`}</h1>\n            <p>Email: {email}</p>\n            <p>Company: {company}</p>\n            <p>Skill: {skill}</p>\n            <p>Average: {gradeAvg}%</p>\n            { expand ? <ExpandList /> : ''}\n          </div>\n        </div>\n        <div className='expand-button-container'>\n          <FontAwesome name={ expand ? 'minus' : 'plus' } size='2x' style={{ color : 'gray' }} onClick={handleClick}/>\n        </div>\n      </div>\n      <hr></hr>\n    </article>\n  )\n}\n\nexport default StudentCard"},"lineCount":null}},"error":null,"hash":"0c2a1cf64d66f3038a41dee55ebf06b5","cacheData":{"env":{}}}